local ScreenGui = Instance.new("ScreenGui")
local ImageLabel = Instance.new("ImageLabel")

local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
ScreenGui.Parent = playerGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false

ImageLabel.Parent = ScreenGui
ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ImageLabel.BackgroundTransparency = 1.0
ImageLabel.Position = UDim2.new(-0.127, 0, 0.408, 0)
ImageLabel.Size = UDim2.new(0, 90, 0, 90)
ImageLabel.Image = "rbxassetid://139336001646889"
ImageLabel.AnchorPoint = Vector2.new(0.5, 0.5)

local img = ImageLabel

wait(0.6)
img:TweenPosition(UDim2.new(0.465, 20, 0.440, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Linear, 0.3, false)
wait(3)
img:TweenPosition(UDim2.new(0.01, 0, 0.813, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Sine, 0.9, false)
